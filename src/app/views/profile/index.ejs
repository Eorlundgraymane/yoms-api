<div class="d-flex justify-content-center">
    <h1>Profile</h1>
</div>
<div class="d-flex flex-column">
    <div class="row">
        <h1>Beneficiaries</h1>
    </div>    
    <div class="row">
        <table class="table table-bordered">
            <thead>
                <th>Number</th>                      
                <th>Pay To</th>
                <% user.beneficiaries.forEach(beneficiary=> {%>
                    <tr>
                        <td>
                            <span><%=beneficiary.accountID%></span>
                        </td>                                      
                        <td>
                            <button onclick="payToAccount('<%=beneficiary.accountID%>')" class="btn btn-sm btn-info" type="button">Pay To</button>
                        </td>        
                    </tr>
    
                    <%})%>
            </thead>
        </table>
    </div>
    
</div>