<div class="table-responsive">
    <table class="table table-hover">
        <tr>
            <td>
                ID : <%=account.ID%>
            </td>
        </tr>
        <tr>
            <td>
                Account name : <%=account.accountName%>
            </td>
        </tr>
        <tr>
            <td>
                Role : <%=account.balance%>
            </td>
        </tr>
        <tr>
            <td>
                Status : <% if(account.isActive){%>Open<%}else{%>Closed<%}%>
            </td>
        </tr>     
    </table>
</div>